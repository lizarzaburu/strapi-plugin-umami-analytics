{
  "name": "@lizarzaburu/strapi-plugin-umami-analytics",
  "version": "1.1.0",
  "author": "Dennis Lizarzaburu",
  "description": "Strapi plugin to display Umami analytics in admin panel with permission-based access control and automatic CSP configuration",
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:lizarzaburu/strapi-plugin-umami-analytics.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "strapi",
    "strapi-plugin",
    "umami",
    "analytics",
    "admin-panel",
    "strapi-5"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "source": "./admin/src/index.ts",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "source": "./server/index.ts",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "admin",
    "server",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.31.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/typescript-utils": "^5.31.3",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.31.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "umami-analytics",
    "displayName": "Umami Analytics",
    "description": "Display Umami analytics with permission-based access control"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}

